@using IMIS_CORE.Core
@model IMIS_Service.ViewModel.ItemMasterVM
@using IMIS_CORE.EnumHelper
<div>
    <div class="form-group">

        <div class="col-md-6">
            <label for="code" class="col-md-3 control-label" control-label">
                @Utils.GetLabel("Fuel Repair Details")<span class="requried">*</span>
            </label>
            <div class="col-md-6">
                <select class="form-control" asp-for="VehiclePartsId" asp-items="Model.fuelmaintenance">
                </select>
            </div>
        </div>
        <div class="col-md-6">
            <label for="code" class="col-md-3 control-label" control-label">
                @Utils.GetLabel("Inv Acc No")<span class="requried">*</span>
            </label>
            <div class="col-md-6">
                <input type="text" class="form-control" asp-for="Code">
            </div>
        </div>
    </div>
    <div class="form-group">

        <div class="col-md-6">
            <label for="code" class="col-md-3 control-label" control-label">
                @Utils.GetLabel("Description")<span class="requried">*</span>
            </label>
            <div class="col-md-6">
                <input type="text" class="form-control" asp-for="NameNp" onkeypress="return setUnicode(event,this);" />
            </div>
        </div>
        <div class="col-md-6">
            <label for="code" class="col-md-3 control-label" control-label">
                @Utils.GetLabel("In English")<span class="requried">*</span>
            </label>

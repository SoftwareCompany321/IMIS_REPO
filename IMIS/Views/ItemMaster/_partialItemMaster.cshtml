            </div>
            <div class="col-md-4">
                <select class="form-control" asp-for="ItemMainClass" asp-items="Model.ItemCategorylist">
                </select>
            </div>

        </div>
    </div>

    <div class="form-group">
        <div class="col-md-6">
            <label for="code" class="col-md-3 control-label" control-label">
                @Utils.GetLabel("Inv Type")<span class="requried">*</span>
            </label>

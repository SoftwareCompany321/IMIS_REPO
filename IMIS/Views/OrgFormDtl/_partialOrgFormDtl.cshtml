            </label>
            <div class="col-md-6">
                <input type="text" class="form-control" asp-for="VatNo">
            </div>
        </div>
        <div class="form-group is-empty">
            <label for="short-name" class="col-md-2 control-label">
                @Utils.GetLabel("Related Person")
            </label>
            <div class="col-md-6">
                <input type="text" class="form-control" asp-for="RelatedPerson">
            </div>
        </div>
        <div class="form-group is-empty">
            <label for="short-name" class="col-md-2 control-label">
                @Utils.GetLabel("Borrow/Lend")
            </label>
            <div class="col-md-6">
                <input type="text" class="form-control" asp-for="Sapati">
            </div>
        </div>
    </div>
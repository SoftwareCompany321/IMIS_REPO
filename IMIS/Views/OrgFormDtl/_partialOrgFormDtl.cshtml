            </div>

        </div>
        <div class="form-group is-empty">
            <label for="short-name" class="col-md-2 control-label">
                @Utils.GetLabel("Mobile No.")
            </label>
            <div class="col-md-6">
                <input type="text" class="form-control" asp-for="MobileNo">
            </div>
        </div>
        <div class="form-group is-empty">
            <label for="short-name" class="col-md-2 control-label">
                @Utils.GetLabel("E-mail")
            </label>
            <div class="col-md-6">
                <input type="text" class="form-control" asp-for="Emails">
            </div>
        </div>
        <div class="form-group is-empty">
            <label for="short-name" class="col-md-2 control-label">
                @Utils.GetLabel("Firm Reg No.")
            </label>
            <div class="col-md-6">
                <input type="text" class="form-control" asp-for="RegNo">
            </div>
        </div>
        <div class="form-group is-empty">
            <label for="short-name" class="col-md-2 control-label">
                @Utils.GetLabel("PAN No.")

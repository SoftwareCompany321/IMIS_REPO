@using IMIS_CORE.Core
@{
    ViewData["Title"] = "ProjectSetupCreate";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<body>
    <div id="page-wrapper">
        <div class="container-fluid">

            <div class="tab-lists-wrapper boxed parentTabs">
                <ul class="nav nav-tabs">
                    <li class="active"><a href="#sub11">Project Setup List</a> </li>

                </ul>

                <div class="tab-content">
                    <a href="@Url.Content("~/ProjectSetupList.html")" class="btn btn-warning btn-fab"><i class="material-icons">arrow_backward</i></a>
                    <div class="tab-pane fade active in" id="sub111">
                        <h2 class="form-title">Add Region</h2>
                        <div class="row">
                            <div class="col-md-8">
                                <div class="well">
                                    <form action="#">
                                        <div class="form-group">
                                            <label for="code" class="col-md-2 control-label">Code <span class="requried">*</span></label>

                                            <div class="col-md-9">
                                                <input type="email" class="form-control" id="code" placeholder="Code">
                                            </div>
                                        </div>
                                        <div class="form-group">
                                            <label for="country" class="col-md-2 control-label">Country</label>
                                            <div class="col-md-2">
                                                <input type="text" class="form-control" id="country-code">
                                            </div>
                                            <div class="col-md-7">
                                                <select name="country" id="country" class="form-control">
                                                    <option value="">Select Country</option>
                                                    <option value="Swaziland">Swaziland</option>
                                                </select>
                                            </div>
                                            <div class="col-md-1"><a href="#" class="btn btn-default form-modal-call" data-toggle="modal" data-target="#search-country"><i class="material-icons">arrow_forward</i></a></div>
                                        </div>
                                        <div class="form-group is-empty">
                                            <label for="region" class="col-md-2 control-label">Region <span class="requried">*</span></label>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" id="region">
                                            </div>
                                        </div>
                                        <div class="form-group is-empty">
                                            <label for="short-name" class="col-md-2 control-label">Short Name </label>
                                            <div class="col-md-9">
                                                <input type="text" class="form-control" id="short-name">
                                            </div>
                                        </div>
                                        <div class="form-group form-action">
                                            <div class="col-md-10 col-md-offset-2">
                                                <button type="submit" class="btn btn-primary">Submit</button>
                                                <button type="button" class="btn btn-default">Cancel</button>
                                            </div>
                                        </div>
                                    </form>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- /.container-fluid -->
    </div>
</body>
